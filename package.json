{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "yarn": ">=1.7.0",
    "node": ">=9.0.0"
  },
  "scripts": {
    "start": "yarn workspace @essence/essence-constructor-website run start",
    "start-localhost": "yarn workspace @essence/essence-constructor-website run start-localhost",
    "build": "yarn workspace @essence/essence-constructor-website run build && cp -r ./packages/@essence/essence-constructor-website/build ./build",
    "CI": "yarn flow stop && concurrently \"yarn lint\" \"yarn flow\" ",
    "precommit": "lint-staged",
    "prepush": "flow",
    "postinstall": "node scripts/postinstall.js",
    "eslint": "eslint",
    "lint": "eslint src license-checker.js packages/@essence/essence-constructor-components/src packages/@essence/essence-constructor-components/mocks packages/@essence/essence-constructor-website/src --ext js,jsx",
    "lint:table": "eslint src --ext js,jsx --format table",
    "license-checker": "node license-checker.js",
    "test": "node scripts/test.js --env=jsdom",
    "test:snapshot": "node scripts/test.snapshot.js --config=jest.snapshot.config.js",
    "chrome:mac": "/Applications/Google\\ Chrome.app/Contents/MacOS/Google\\ Chrome --remote-debugging-port=9222 --user-data-dir=chrome",
    "chrome:win": "<path to chrome>/chrome.exe --remote-debugging-port=9222 --user-data-dir=chrome",
    "chrome:linux": "google-chrome --remote-debugging-port=9222 --user-data-dir=chrome",
    "generate-documentation": "node scripts/generateDocumentation.js"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "eslint",
      "git add"
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "7.1.0",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/runtime": "7.0.0-beta.49",
    "@types/jest": "^24.0.9",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "babel-preset-react-app": "^5.0.4",
    "concurrently": "^4.0.1",
    "cross-spawn": "^6.0.5",
    "documentation": "^10.0.1",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.7.0",
    "eslint": "^4.10.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-config-react-app": "2.1.0",
    "eslint-plugin-filenames": "1.3.2",
    "eslint-plugin-flowtype": "2.46.1",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-prettier": "2.6.0",
    "eslint-plugin-react": "7.10.0",
    "eslint-plugin-react-app": "3.0.0",
    "flow-bin": "0.79.1",
    "fs-extra": "^7.0.1",
    "husky": "^0.14.3",
    "jest": "^23.6.0",
    "jest-image-snapshot": "^2.7.0",
    "jsdoc": "^3.6.2",
    "jsdoc-babel": "^0.5.0",
    "license-checker": "^20.1.0",
    "lint-staged": "^7.0.0",
    "node-fetch": "^2.6.0",
    "object-assign": "4.1.1",
    "prettier": "^1.10.2",
    "promise": "8.0.1",
    "prompt": "^1.0.0",
    "puppeteer": "^1.10.0",
    "raf": "3.4.0",
    "stream-array": "^1.1.2",
    "tslint": "^5.13.1",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "vinyl-fs": "^3.0.3"
  },
  "workspaces": {
    "packages": [
      "packages/@essence/essence-constructor-components",
      "packages/@essence/essence-constructor-website",
      "packages/@essence/essence-constructor-simple-module"
    ]
  },
  "dependencies": {}
}
